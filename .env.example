# Example env vars
FIREHOSE_WS_URL=wss://bsky.network/xrpc/com.atproto.sync.subscribeRepos
LABELER_ENDPOINT=https://labeler.example/xrpc/com.atproto.label.queryLabels
# Rate limiting for outgoing label queries (requests per second)
LABEL_QUERY_RATE=5
# Max concurrent outgoing label queries
LABEL_QUERY_CONCURRENCY=3
# Retries and backoff
LABEL_QUERY_RETRIES=3
LABEL_QUERY_BACKOFF_BASE=0.5
LABEL_QUERY_BACKOFF_MAX=5.0

# Admin API token (optional). If set, admin endpoints require this token in the
# `Authorization: Bearer <token>` header or in the `X-Admin-Token` header.
ADMIN_API_TOKEN=

FIREHOSE_AUTO_START=0
